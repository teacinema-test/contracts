{
  "name": "@teacinema-test/contracts",
  "version": "1.0.1",
  "description": "Protobuf definitions and generated TypeScript types",
  "scripts": {
    "generate:win": "protoc -I proto --plugin=protoc-gen-ts_proto=node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=gen --ts_proto_opt=nestJs=true,package=omit proto\\*.proto",
    "generate:unix": "protoc -I proto --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=gen --ts_proto_opt=nestJs=true,package=omit proto/*.proto"
  },
  "files": [
    "proto",
    "gen"
  ],
  "devDependencies": {
    "grpc-tools": "^1.13.1",
    "ts-proto": "^2.11.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/microservices": "^11.1.14",
    "rxjs": "^7.8.2"
  }
}
